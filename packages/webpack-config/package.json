    "fs-extra": "^11.2.0",
    "getenv": "^1.0.0",
    "html-webpack-plugin": "^5.5.0",
    "is-wsl": "^2.0.0",
    "mini-css-extract-plugin": "^2.5.2",
    "node-html-parser": "^5.2.0",
    "semver": "~7.5.4",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^4.1.1"
  },
