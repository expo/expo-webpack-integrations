{
  "preferGlobal": true,
  "name": "expo-pwa",
  "version": "0.0.127",
  "author": "Expo <support@expo.dev>",
  "description": "Create PWA resources for Expo projects",
  "license": "MIT",
  "keywords": [
    "expo",
    "pwa",
    "react-native",
    "react-native-web"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "expo-pwa": "./build/cli.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "expo-module tsc",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "prepare": "expo-module clean && expo-module tsc",
    "test": "expo-module test",
    "typecheck": "expo-module typecheck",
    "watch": "expo-module-tsc --watch --preserveWatchOutput"
  },
  "homepage": "https://github.com/expo/expo-webpack-integrations/tree/main/packages/pwa#readme",
  "bugs": {
    "url": "https://github.com/expo/expo-webpack-integrations/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-webpack-integrations.git",
    "directory": "packages/pwa"
  },
  "dependencies": {
    "@expo/image-utils": "0.3.23",
    "chalk": "^4.0.0",
    "commander": "2.20.0",
    "update-check": "1.5.4"
  },
  "devDependencies": {
    "expo-module-scripts": "^3.4.0"
  },
  "peerDependencies": {
    "expo": "*"
  },
  "publishConfig": {
    "access": "public"
  }
